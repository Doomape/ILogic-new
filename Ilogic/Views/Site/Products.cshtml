<div id="products" style="background-color: white; width: 100%"></div>

<script type="text/javascript">

    $(window).resize(function () {
        var height = parseInt($(document).height() - 230);
        $("#products").css("height", height);
    });
    $(document).ready(function () {
        $("#top-row").empty();
        var height = parseInt($(document).height() - 230);
        $("#products").css("height", height);
        $("html").css("height", height);
        //$("#logoImages").css("height", height);
        $('#bottom-row').css('background', 'white');
        $('#top-row').css('background', '#979dd7');
        document.body.style.background = "white";
    });
    function showImage() {
        $("#products").empty();
        $("#products").append("<img src='/Content/images/Sumo_Robot(1).JPG'/>");
    }
</script>

