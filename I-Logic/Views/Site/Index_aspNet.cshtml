@{
    ViewBag.Title = "Index_aspNet";
    Layout = "~/Views/Shared/SiteMaster.cshtml";
}
<style>
    p:first-letter {
        color: red;
    }
   .ui-dialog.ui-widget-content { background: #007acc; }
   .ui-corner-all { border-radius:0px 0px}
   .ui-dialog-titlebar { background: #2d2d30;border: 0px;padding-bottom:20px !important}
   .ui-dialog-title {color: gray;font-size: 12px;margin-left: 35px !important;padding-top: 4px;}
   .ui-dialog-content.ui-widget-content
   {
       border-left: 13px solid #333333;
       border-right: 13px solid #333333;
       border-bottom: 13px solid #333333;
   }
   .ui-button.ui-widget.ui-state-default.ui-corner-all.ui-button-icon-only.ui-dialog-titlebar-close
   {
       position: absolute;
        top: 20px;
        right: 15px;
   }
   .visual_logo 
   {
       width:20px;height:20px;background:url('/Content/images/visual_icon.png');display:block;position:absolute;
   }
   .visual_buttons
   {
       margin-left:30px;position:absolute;left:225px;top:25px;
   }
   .buttons
   {
       float:left;display:block;width:15px;height:15px;cursor:pointer;
   }
</style>
<div id="aspNet" style="background-color:rgb(45,45,48);width:100%">
    <div id="consoleSpace" style="float:left;width:600px;height:100%">
        <div id="console" title="Create Album" style="overflow: hidden;min-height: 173px !important"></div>
    </div>
    <div id="idDiv" style="float:left">
        <img src="/Content/images/MicrosoftSQL_Server.png" />
        <div id="coverMouse" style="width:200px;height:200px;float: left;margin-left: -120px;margin-top: 7px;background-color: white;width: 150px;height: 150px;opacity: 0.3;color: black;display: none;">
            <p>ON MOUSEEEEEEEEEE</p>
        </div>
    </div>
</div>



  <script type="text/javascript">

    
      $(window).resize(function () {
          var height = parseInt($(document).height() - 230);
          $("#aspNet").css("height", height);
      });



      $(document).ready(function () {
          var height=parseInt($(document).height()-230);
          $("#aspNet").css("height", height);
          $('#bottom-row').css('background', 'rgb(30,30,30)');
          $('#top-row').css('background', 'rgb(30,30,30)');
          document.body.style.background = "#282828";
          $("#idDiv").mouseover(function () {
              $("#coverMouse").show();
          }).mouseout(function () {
              $("#coverMouse").hide();
          });
      });

      $(function () {
          var dialog = $('#console').dialog({
              autoOpen: true,
              width: 540,
              resizable: false,
              title: 'Microsoft Visual Studio',
              modal: false,
              open: function (event, ui) {
                  //Load the CreateAlbumPartial action which will return 
                  // the partial view _CreateAlbumPartial
                  $(this).load("@Url.Action("PartialConsoleWindow")");
            },
            //buttons: {
            //    "Close": function () {
            //        $(this).dialog("close");
            //    }
            //}
        });
          $(".ui-dialog-titlebar").prepend("<div class='visual_logo'></div>");
          $(".ui-dialog-titlebar").prepend("<div class='visual_buttons'>"+
            "<div class='buttons' onclick='javascript:alert(1);' style='background:url(/Content/images/debug.png) no-repeat'></div>" +
            "<div class='buttons' style='background:url(/Content/images/pause.png) no-repeat'></div>" +
            "<div class='buttons' style='background:url(/Content/images/stop.png) no-repeat'></div>" +
          "</div>");

          dialog.parent().find('a').find('span').attr('class', 'ui-icon ui-icon-minus');
          dialog.parent().draggable({
              containment: '#consoleSpace',
              opacity: 0.70
          });

          var target = $("#consoleSpace");
          $("#console").dialog("widget").position({
              my: 'left bottom',
              at: 'left middle',
              of: target
          });

          $('#consoleSpace').append(dialog.parent());
          //$('#my-button').click(function () {
          //    $('#dialog').dialog('open');
          //});
      });

      

</script>